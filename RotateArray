class Solution {
    public void rotate(int[] nums, int k) {
        int count = 0; 
        while(count<k){
            int temp = nums[0];
            int temp2 = nums[nums.length-1]; 
            for(int i=nums.length-1; i>=1; i--){
                nums[i]=nums[i-1]; 
            }
            nums[0]=temp2;
            //nums[nums.length-1]= temp; 
            count++; 

       }
    }
}
